{
  "rules": {
    "security_logs": {
      ".read": false,
      ".write": true
    },
    "games": {
      ".read": true,
      ".write": true
    },
    "status": {
      ".read": true,
      ".write": true
    },
    "messages": {
      ".read": true,
      ".write": true,
      "$messageId": {
        ".validate": "newData.hasChildren(['sender', 'receiver', 'text', 'timestamp']) || newData.hasChildren(['seen'])"
      }
    },
    "tokens": {
      ".read": false,
      ".write": true
    }
  }
}
